{
  "plugins": ["jsdoc-http-plugin"],
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs|seeders|migrations)"
  },
  "templates": {
      "name": "Overlord.js",
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "template": "node_modules/docdash",
    "destination": "./doc/overlord.js",
    "readme": "./README.md"
  }
}
